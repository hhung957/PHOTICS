<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edit</servlet-name>
        <servlet-class>Edit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>avatar</servlet-name>
        <servlet-class>avatar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Forgot</servlet-name>
        <servlet-class>Forgot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editpass</servlet-name>
        <servlet-class>Editpass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Deleteacc</servlet-name>
        <servlet-class>Deleteacc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchA</servlet-name>
        <servlet-class>SearchA</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>avatar</servlet-name>
        <url-pattern>/avatar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edit</servlet-name>
        <url-pattern>/Edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Forgot</servlet-name>
        <url-pattern>/Forgot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editpass</servlet-name>
        <url-pattern>/Editpass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Deleteacc</servlet-name>
        <url-pattern>/Deleteacc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchA</servlet-name>
        <url-pattern>/SearchA</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
